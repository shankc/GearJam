<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />




    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="toolbarStyle">@style/Widget.AppCompat.Toolbar</item>
        <item name="colorPrimary">#3F51B5</item>

        <!-- colorPrimaryDark is used for coloring the status bar -->
        <item name="colorPrimaryDark">#303F9F</item>

        <!-- colorAccent is used as the default value for colorControlActivated
             which is used to tint widgets -->
        <item name="colorAccent">#FFAB40</item>

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="SeekBarseekbar" parent="android:Widget.Holo.SeekBar">

    </style>

    <style name="PhotoGrid">
        <item name="android:drawSelectorOnTop">true</item>
        <item name="android:listSelector">@drawable/photogrid_selector</item>
    </style>




</resources>
